<!DOCTYPE html>
<html lang="en">
<!-- Head: DON'T DELETE -->
<% include partials/head %>

<body class="body-meeting-room">

  <% include partials/video-controller-floating %>



  <div id="header">
    <a target="_blank" href="/">
      <img src="/images/logo.svg" alt="Neon" width="48" height="48" />
      <p>Zipcall</p>
    </a>
  </div>

  <p id="remote-video-text"></p>
  <video id="remote-video" autoplay playsinline></video>
  <div id="moveable">
    <p id="local-video-text">No webcam input</p>
    <video id="local-video" autoplay muted playsinline></video>
  </div>

  <div id="entire-chat">
    <div id="chat-zone">
      <div class="chat-messages" id="chat-messages-box">
        <!-- msg1 example -->
        <!-- <div class="message-item customer cssanimation fadeInBottom">
          <div class="message-bloc">
