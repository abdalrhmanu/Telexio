              self msg
            </div>
          </div>
        </div> -->

        <!-- msg2 example -->
        <!-- <div class="message-item moderator cssanimation fadeInBottom">
          <div class="message-bloc">
            <div class="message">
              From: user msg
            </div>
          </div>
        </div> -->

      </div>
    </div>
    <form class="compose" id="chat-form" autocomplete="off">
      <input type="text" placeholder="Type a message" id="msg" required/>
    </form>
  </div>

  <% include partials/meeting-details %>
  
  <!-- <script src="https://unpkg.com/peerjs@1.3.1/dist/peerjs.min.js"></script> -->
  <script src="/socket.io/socket.io.js"></script>
